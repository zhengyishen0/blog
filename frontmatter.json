{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "author",
          "name": "author",
          "type": "string"
        },
        {
          "title": "date",
          "name": "date",
          "type": "datetime"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "pin",
          "name": "pin",
          "type": "boolean"
        },
        {
          "title": "img_path",
          "name": "img_path",
          "type": "string"
        },
        {
          "title": "image",
          "name": "image",
          "type": "image",
          "isPreviewImage": true
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": "assets/img",
  "frontMatter.preview.host": "http://localhost:4000",
  "frontMatter.taxonomy.tags": [
    "buildinpublic",
    "entrepreneurship",
    "indiehackers",
    "lean startup",
    "saas",
    "web development"
  ],
  "frontMatter.taxonomy.categories": ["30daychallenge"],
  "frontMatter.content.pageFolders": [
    {
      "title": "_posts",
      "path": "[[workspace]]/_posts",
      "previewPath": "'posts'"
    }
  ],
  "frontMatter.git.enabled": true
}
