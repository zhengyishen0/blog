{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "previewPath": null,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Content preview",
          "name": "image",
          "type": "image",
          "isPreviewImage": true
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "Draft",
          "name": "published",
          "type": "draft"
        }
      ]
    }
  ],
  "frontMatter.framework.id": "jekyll",
  "frontMatter.content.publicFolder": "assets/img",
  "frontMatter.preview.host": "http://localhost:4000",
  "frontMatter.taxonomy.tags": [
    "buildinpublic",
    "entrepreneurship",
    "indiehackers",
    "lean startup",
    "saas",
    "web development"
  ],
  "frontMatter.taxonomy.categories": ["30daychallenge"],
  "frontMatter.content.pageFolders": [
    {
      "title": "_posts",
      "path": "[[workspace]]/_posts",
      "previewPath": "'posts'"
    }
  ],
  "frontMatter.git.enabled": true
}
